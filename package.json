{
  "name": "@supabase/realtime-js",
  "version": "0.0.0",
  "description": "Listen to realtime updates to your PostgreSQL database",
  "keywords": [
    "realtime",
    "phoenix",
    "elixir",
    "javascript",
    "typescript",
    "firebase",
    "supabase"
  ],
  "homepage": "https://github.com/supabase/realtime-js",
  "bugs": "https://github.com/supabase/realtime-js/issues",
  "files": [
    "dist"
  ],
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "repository": "https://github.com/supabase/realtime-js",
  "author": "Supabase",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist docs",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "build": "run-s clean format build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "mocha": "node -r esm node_modules/.bin/mocha --exit ./test/**/*.js -r jsdom-global/register",
    "test": "run-s clean build mocha",
    "docs": "typedoc --mode file --target ES6 --theme minimal",
    "docs:json": "typedoc --json docs/spec.json --mode modules --includeDeclarations --excludeExternals"
  },
  "dependencies": {
    "query-string": "^6.12.1",
    "websocket": "^1.0.31"
  },
  "devDependencies": {
    "@babel/runtime": "^7.9.2",
    "@supabase/doctest-js": "^0.1.0",
    "@types/websocket": "^1.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "eslint": "^7.0.0",
    "esm": "^3.2.25",
    "jsdom": "16.4.0",
    "jsdom-global": "3.0.0",
    "mocha": "^8.0.1",
    "mock-socket": "^9.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "sinon": "^9.0.2",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.3"
  }
}
